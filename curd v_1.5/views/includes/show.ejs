<% let i=0 %>
    <% for(let song of songs) {%>
        <tr class="table-dark text-center">
            <th scope="row">
                <%= i=i+1 %>
            </th>
            <td scope="row">
                <%= song.categoryId %>
            </td>
            <td>
                <%= song.title %>
            </td>
            <td>
                <%= song.artist %>
            </td>
            <td>
                <a href="<%= song.imageUrl %>" target="_blank">
                    <%= song.imageUrl %>
                </a>
            </td>
            <% if(isAdmin === 'isAdmin') { %> 
            <td>
                <a href="/admin/update-song/<%= song._id %>?edit=true" type="button"><i
                        class="far fa-edit"></i></a>
                <form action="/admin/delete-song" method="post">
                    <input type="hidden" value="<%= song._id %>" name="songId">
                    <button class="delete" type="submit" id="<%= song._id %>"><i class="far fa-trash"></i></button>
                </form>
            </td>
            <% } %> 
        </tr>
        <% } %>