<% for(let song of songs)  {%>
    <tr class="table-primary text-center">
        <th  scope="row"><%= song.categoryId.title %></th>
        <td><%= song.title %></td>
        <td><%= song.artist %></td>
        <td>
            <a href="<%= song.imageUrl %>"><%= song.imageUrl %></a>
        </td>
        
        <td>
            <a href="/admin/update-song/<%= song._id %>&<%= song.categoryId._id %>?edit=true" type="button"><i class="far fa-edit"></i></a>
            <!-- <a href="/user/delete-song" id="<%= song._id %>" type="button" ><i class="far fa-trash"></i></a> -->
      
        <form action="/admin/delete-song" method="post">
            <input type="hidden" value="<%= song._id %>" name="songId">
          <button style="border: none; background-color: transparent;" type="submit" id="<%= song._id %>"><i class="far fa-trash"></i></button>
        </form>
    </td>
    </tr>
<% } %>